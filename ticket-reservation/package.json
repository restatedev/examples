{
  "name": "ticket-reservation",
  "version": "0.0.1",
  "description": "An example to illustrate how Restate's keyed-sharding and concurrency guarantees simplify microservice architectures",
  "main": "app.js",
  "type": "commonjs",
  "license": "MIT",
  "author": "Restate developers",
  "email": "info@restate.dev",
  "scripts": {
    "lint": "eslint --ignore-path .eslintignore --ext .ts .",
    "format": "prettier --ignore-path .eslintignore --write \"**/*.+(js|ts|json)\"",
    "build": "tsc --noEmitOnError",
    "app": "node ./dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only ./src/app.ts"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.0",
    "esbuild": "^0.19.0",
    "@restate-examples/eslint-config": "*",
    "@restate-examples/prettier-config": "*",
    "ts-node-dev": "^2.0.0",
    "uuid": "^9.0.0"
  }
}
