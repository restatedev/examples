  "license": "MIT",
  "email": "code@restate.dev",
  "type": "commonjs",
  "scripts": {
    "build": "tsc --noEmitOnError",
    "puppeteer-service": "ts-node-dev --watch ./src --respawn --transpile-only src/puppeteer/puppeteer.ts",
