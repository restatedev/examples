{
  "name": "@restatedev/example-hello-world-lambda-cdk",
  "version": "0.1.0",
  "bin": {
    "lambda-ts-cdk": "bin/lambda-ts-cdk.js"
  },
  "scripts": {
    "lint": "eslint --ignore-path .eslintignore --ext .ts .",
    "build": "npx cdk synth",
    "format": "prettier --ignore-path .eslintignore --write \"**/*.+(js|ts|json)\"",
    "verify": "npm run format -- --check && npm run lint && npm run build",
    "deploy": "npm run build && lib deploy"
  },
  "devDependencies": {
    "@restatedev/restate-cdk": "^0.7.0",
    "@types/node": "20.9.1",
    "aws-cdk": "^2.121.0",
    "esbuild": "^0.19.11",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@restatedev/restate-sdk": "^0.7.0",
    "aws-cdk-lib": "^2.121.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
